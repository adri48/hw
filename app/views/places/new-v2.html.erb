<%= form_with :model => @place do |form| %>
  <p>
    <%= form.label "place", "Place Name" %>
    <%= form.text_field "place" %>
  </p>
  <p>
    <%= form.label "city" %>
    <%= form.text_field "city" %>
    <%= form.label "state" %>
    <%= form.text_field "state" %>
  </p>
  <%= form.submit %>
<% end %>
